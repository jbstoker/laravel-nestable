{
    "name": "jbstoker/laravel-nestable",
    "type": "library",
    "description": "Laravel 5 nested categories library",
    "homepage": "https://github.com/jbstoker/nestable",
    "keywords": ["nested","nestable","nestedset","category","laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stoker",
            "homepage": "http://www.stokstylez.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.1.2"
    },
    "autoload": {
        "psr-4": {
            "Nestable\\": "src/",
            "Nestable\\Tests\\": "tests/"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nestable\\NestableServiceProvider"
            ],
            "aliases": {
                "Nestable": "Nestable\\Facades\\NestableService"
            }
        }
    }
}
